<fieldset class="bikeDependent">
	<legend>Bike Constants</legend>
  <div class="fieldWrapper">
      <label for="tireWidth">Rear Tire Width (In Millimeters)</label>
      <input type="number" min="0" step="1" name="tireWidth" class="tireWidth" required="required" disabled="disabled" readonly="readonly" placeholder="Rear Tire Width (In Millimeters)" value="{{tireWidth}}"/>
    </div>
    <div class="fieldWrapper">
      <label for="aspectRatio">Rear Tire Aspect Ratio</label>
      <input type="number" min="0" step="1" name="aspectRatio" class="aspectRatio" required="required" disabled="disabled" readonly="readonly" placeholder="Rear Tire Aspect Ratio" value="{{aspectRatio}}"/>
    </div>
    <div class="fieldWrapper">
      <label for="rimSize">Rear Tire Rim Size (In Inches)</label>
      <input type="number" min="0" step="1" name="rimSize" class="rimSize" required="required" disabled="disabled" readonly="readonly" placeholder="Rear Tire Rim Size (In Inches)" value="{{rimSize}}"/>
    </div>
    <div class="fieldWrapper">
		  <label for="tireCirc">Rear Tire Circumference (In Inches)</label>
    	<input type="number" min="0" step="0.001" name="tireCirc" class="tireCirc" required="required" disabled="disabled" readonly="readonly" placeholder="Rear Tire Circumference (In Inches)" value="{{tireCirc}}"/>
  	</div>
  	<div class="fieldWrapper">
  		<label for="primaryDrive">Primary Drive Ratio</label>
    	<input type="number" min="0" step="0.001" name="primaryDrive" class="primaryDrive" required="required" disabled="disabled" readonly="readonly" placeholder="Primary Drive Ratio" value="{{primaryDrive}}"/>
  	</div>
    <div class="fieldWrapper">
      <label for="maxPowerRPM">Max Power RPM</label>
      <input type="number" min="0" step="1" name="maxPowerRPM" class="maxPowerRPM" required="required" disabled="disabled" readonly="readonly" placeholder="Max Power RPM" value="{{maxPowerRPM}}"/>
    </div>
    <div class="fieldWrapper">
      <label for="maxTorqueRPM">Max Torque RPM</label>
      <input type="number" min="0" step="1" name="maxTorqueRPM" class="maxTorqueRPM" required="required" disabled="disabled" readonly="readonly" placeholder="Max Torque RPM" value="{{maxTorqueRPM}}"/>
    </div>
    <div class="fieldWrapper">
      <label for="maxTorque">Max Torque (in ft/lbs)</label>
      <input type="number" min="0" step="1" name="maxTorque" class="maxTorque" required="required" disabled="disabled" readonly="readonly" placeholder="Max Torque (in ft/lbs)" value="{{maxTorque}}"/>
    </div>
  	<div class="fieldWrapper">
  		<label for="redlineRPM">Redline RPM</label>
    	<input type="number" min="0" step="1" name="redlineRPM" class="redlineRPM" required="required" disabled="disabled" readonly="readonly" placeholder="Redline RPM" value="{{redlineRPM}}"/>
  	</div>
  	<div class="fieldWrapper">
  		<label for="wheelBase">Wheel Base (In Millimeters)</label>
    	<input type="number" min="0" step="1" name="wheelBase" class="wheelBase" required="required" disabled="disabled" readonly="readonly" placeholder="Wheel Base (In Millimeters)" value="{{wheelBase}}"/>
  	</div>
</fieldset>
<fieldset class="bikeDependent">
	<legend>Bike Constants (After Upgrades and Tire Pressure)</legend>
	<div class="fieldWrapper">
		<label for="bhp">Break Horsepower (BHP)</label>
  		<input type="number" min="0" step="0.001" name="bhp" class="bhp" required="required" placeholder="Break Horsepower (BHP)" value="{{bhp}}"/>
    </div>
    <div class="fieldWrapper">
    	<label for="weight">Bike Weight (In Pounds)</label>
    	<input type="number" min="0" step="1" name="weight" class="weight" required="required" placeholder="Bike Weight (In Pounds)" value="{{weight}}"/>
  	</div>
  	<div class="fieldWrapper">
  		<label for="tirePressure">Tire Pressure (In Percent)</label>
  		<div class="rangeLabels">
  			<span class="rangeValue">90</span>
  			<span class="rangeValue">91</span>
  			<span class="rangeValue">92</span>
  			<span class="rangeValue">93</span>
  			<span class="rangeValue">94</span>
  			<span class="rangeValue">95</span>
  			<span class="rangeValue">96</span>
  			<span class="rangeValue">97</span>
  			<span class="rangeValue">98</span>
  			<span class="rangeValue">99</span>
  			<span class="rangeValue">100</span>
  			<span class="rangeValue">101</span>
  			<span class="rangeValue">102</span>
  			<span class="rangeValue">103</span>
  			<span class="rangeValue">104</span>
  			<span class="rangeValue">105</span>
  			<span class="rangeValue">106</span>
  			<span class="rangeValue">107</span>
  			<span class="rangeValue">108</span>
  			<span class="rangeValue">109</span>
  			<span class="rangeValue">110</span>
  		</div>
    	<input type="range" min="0.9" max="1.1" defaultValue="1" step="0.01" name="tirePressure" class="tirePressure" required="required" placeholder="Tire Pressure" value="{{tirePressure}}"/>
  	</div>
  	<div class="fieldWrapper">
  		<label for="grip">Tire Grip (In Newtons)</label>
    	<input type="number" min="0" step="1" name="grip" class="grip" required="required" placeholder="Tire Grip (In Newtons)" value="{{grip}}"/>
  	</div>
  	<div class="fieldWrapper">
  		<label for="swingArm">Swingarm Installed</label>
    	<select name="swingArm" class="swingArm" required="required">
    		<option value="0.00" {{#compare swingArm 0.00}}selected="selected"{{/compare}}>Stock Swingarm</option>
    		<option value="0.03" {{#compare swingArm 0.03}}selected="selected"{{/compare}}>Swingarm 1</option>
    		<option value="0.05" {{#compare swingArm 0.05}}selected="selected"{{/compare}}>Swingarm 2</option>
    		<option value="0.06" {{#compare swingArm 0.06}}selected="selected"{{/compare}}>Swingarm 3</option>
    		<option value="0.07" {{#compare swingArm 0.07}}selected="selected"{{/compare}}>Swingarm 4</option>
    	</select>
  	</div>
</fieldset>
	<fieldset class="bikeDependent">
	<legend>Nitrous Settings</legend>
    <div class="fieldWrapper">
    	<label for="nosBHP">NOS Break Horsepower (BHP)</label>
  		<input type="number" min="0" step="1" name="nosBHP" class="nosBHP" required="required" placeholder="NOS Break Horsepower (BHP)" value="{{nosBHP}}"/>
    </div>
    <div class="fieldWrapper">
    	<label for="nosDuration">NOS Duration (In Seconds)</label>
  		<input type="number" min="2.000" max="5.000" step="0.001" name="nosDuration" class="nosDuration" required="required" placeholder="NOS Duration (In Seconds)" value="{{nosDuration}}"/>
    </div>
	</fieldset> 
	<fieldset class="bikeDependent">
	<legend>Sprocket Settings</legend>
	<div class="fieldWrapper">
		<label for="frontSprocket">Front Sprocket Teeth</label>
		<input type="number" name="frontSprocket" class="frontSprocket" required="required" placeholder="Front Sprocket Teeth" min="10" max="19" step="1" value="{{frontSprocket}}"/>
		</div>
		<div class="fieldWrapper">
			<label for="rearSprocket">Rear Sprocket Teeth</label>
		<input type="number" name="rearSprocket" class="rearSprocket" required="required" placeholder="Rear Sprocket Teeth" min="35" max="55" step="1" value="{{rearSprocket}}"/>
		</div>
</fieldset>
<fieldset class="bikeDependent">
	<legend>Gear Settings</legend>
<div class="fieldWrapper">
	<label for="gear1Ratio">Gear 1 Ratio</label>
		<input type="number" name="gear1Ratio" class="gear1Ratio" class="gear" required="required" placeholder="Gear 1 Ratio" min="0.4" step="0.001" max="5.0" value="{{gear1Ratio}}"/>
</div>
<div class="fieldWrapper">
	<label for="gear2Ratio">Gear 2 Ratio</label>
		<input type="number" name="gear2Ratio" class="gear2Ratio" class="gear" required="required" placeholder="Gear 2 Ratio" min="0.4" step="0.001" max="5.0" value="{{gear2Ratio}}"/>
</div>
<div class="fieldWrapper">
	<label for="gear3Ratio">Gear 3 Ratio</label>
		<input type="number" name="gear3Ratio" class="gear3Ratio" class="gear" required="required" placeholder="Gear 3 Ratio" min="0.4" step="0.001" max="5.0" value="{{gear3Ratio}}"/>
</div>
<div class="fieldWrapper">
	<label for="gear4Ratio">Gear 4 Ratio</label>
		<input type="number" name="gear4Ratio" class="gear4Ratio" class="gear" placeholder="Gear 4 Ratio" min="0.4" step="0.001" max="5.0" value="{{gear4Ratio}}"/>
		<a href="#" class="disableInputButton" title="Disable Gear">Disable</a>
</div>
<div class="fieldWrapper">
	<label for="gear5Ratio">Gear 5 Ratio</label>
		<input type="number" name="gear5Ratio" class="gear5Ratio" class="gear" placeholder="Gear 5 Ratio" min="0.4" step="0.001" max="5.0" value="{{gear5Ratio}}"/>
		<a href="#" class="disableInputButton" title="Disable Gear">Disable</a>
</div>
<div class="fieldWrapper">
	<label for="gear6Ratio">Gear 6 Ratio</label>
		<input type="number" name="gear6Ratio" class="gear6Ratio" class="gear" placeholder="Gear 6 Ratio" min="0.4" step="0.001" max="5.0" value="{{gear6Ratio}}"/>
		<a href="#" class="disableInputButton" title="Disable Gear">Disable</a>
</div>
</fieldset>